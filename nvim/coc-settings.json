{
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "typescript",
        "typescriptreact",
        "json",
        "javascriptreact",
        "typescript.tsx"
    ],
    "eslint.filetypes": [
        "javascript",
        "typescript",
        "typescriptreact",
        "javascriptreact",
        "typescript.tsx"
    ],
    "coc.preferences.diagnostic.virtualText": true,

    "explorer.file.showHiddenFiles": true,
    "explorer.width": 30,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings": {
        "<cr>": [
            "expandable?",
            [
                "expanded?",
                "collapse",
                "expand"
            ],
            "open"
        ],
        "v": "open:vsplit"
    }
}